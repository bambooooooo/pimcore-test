<div style="position: relative; z-index: 150;">
    {% if item.data.QuantityDone is defined and item.data.QuantityDone > 0 %}
        <img src="data:image/svg+xml;base64," height="18">
        <span id="li-quantity-{{ p.id }}-{{ serie.id }}-{{ item.data.Quantity }}-{{ loop.index }}">
            {{ item.data.QuantityDone }}
        </span>
        {% if item.data.QuantityDone != item.data.Quantity %}
            <span id="li-quantity-source-{{ p.id }}-{{ serie.id }}-{{ item.data.Quantity }}-{{ loop.index }}" style="text-decoration: line-through">
                {{ item.data.Quantity }}
            </span>
        {% endif %}
    {% else %}
        <span id="li-quantity-{{ p.id }}-{{ serie.id }}-{{ item.data.Quantity }}-{{ loop.index }}">
            {{ item.data.Quantity }}
        </span>
        <div class="mb-1">
            <input type="number" style="width: 50px; display: none;" min="0" value="{{ item.data.Quantity }}" id="li-done-input-{{ p.id }}-{{ serie.id }}-{{ item.data.Quantity }}-{{ loop.index }}"/>
            <span class="btn btn-outline-success btn-sm li-done-toggle" data-id="{{ p.id }}-{{ serie.id }}-{{ item.data.Quantity }}-{{ loop.index }}">
                <img height="18" src="data:image/svg+xml;base64," />
            </span>
        </div>
    {% endif %}
</div>