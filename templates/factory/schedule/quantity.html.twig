<div style="position: relative; z-index: 150;">
    {% if item.data.QuantityDone is defined and item.data.QuantityDone > 0 and item.data.QuantityDone != item.data.Quantity %}
        <img src="data:image/svg+xml;base64," height="18">
        {{ item.data.QuantityDone }}
        <span style="text-decoration: line-through">
            {{ item.data.Quantity }}
        </span>
    {% else %}
        {{ item.data.Quantity }}
    {% endif %}
</div>