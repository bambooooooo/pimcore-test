<div>
    {% if app.user.schedule_show_receiver %}
        <a href="{{ path('factory_tree', {'id': serie.id}) }}" {% if not serie.published and type != "pdf" %}style="text-decoration: line-through"{% endif %}>
            {{ user.key }}
        </a>
        <div>
            {% if serie.ExternalNumber %}
                {{ serie.ExternalNumber }}
            {% endif %}
        </div>
    {% else %}
        ****
    {% endif %}

    {% if type != 'pdf' and app.user.schedule_show_item_checkbox %}
        <br/>
        <input type="checkbox" class="cb-status"
               id="serie-checkbox-{{ serie.id }}"
               name="serie-checkbox-{{ serie.id }}">
    {% endif %}
</div>

{% if type != "pdf" %}
    {% if app.user.schedule_print_control_card %}
        <div class="btn-group ms-2">
            <button class="btn btn-secondary btn-sm dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                <img src="/bundles/pimcoreadmin/img/flat-white-icons/support.svg" height="16"/>
            </button>
            <ul class="dropdown-menu">
                <li><a class="dropdown-item" href="{{ path("factory_elements", {id: serie.id}) }}">Drukuj karty kontrolne (test)</a></li>
            </ul>
        </div>
    {% endif %}

    {# Buttons not used in this version #}
    <div class="m-1 d-none">
        <button class="btn btn-outline-secondary serie-cutting-btn"
                type="button" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Rozkroje" data-id="{{ serie.id }}">
            <img src="/bundles/pimcoreadmin/img/flat-color-icons/template.svg" height="20" width="20">
            Rozkroje
        </button>
    </div>
    <div class="m-1 d-none">
        <button class="btn btn-outline-secondary serie-borders-btn"
                type="button" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Materiały" data-id="{{ serie.id }}">
            <img src="/bundles/pimcoreadmin/img/flat-color-icons/survey.svg" height="20" width="20">
            Obrzeża
        </button>
    </div>
    <div class="m-1 d-none">
        <button class="btn btn-outline-secondary serie-supplies-btn"
                type="button" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Rozkroje" data-id="{{ serie.id }}">
            <img src="/bundles/pimcoreadmin/img/flat-color-icons/workflow.svg" height="20" width="20">
            Dostawy
        </button>
    </div>
    {% if app.user.schedule_order_sheets %}
        <div class="m-1">
            <button class="btn btn-outline-secondary serie-sheets-btn"
                    type="button" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Arkusze"
                    data-id="{{ serie.id }}" data-label="{{ order.key }}-{{ serie.key }}">
                <img src="/UI/loading.gif" height="20" width="20" id="serie-sheets-spinner-{{ serie.id }}" style="display: none">
                <img src="data:image/svg+xml;base64," id="serie-sheets-icon-{{ serie.id }}" height="20" width="20">
                Arkusze
            </button>
        </div>
    {% endif %}
{% endif %}